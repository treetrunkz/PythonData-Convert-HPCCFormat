                    <xsd:element name="VehicleCpo"/>
                    <xsd:element name="ConnectedServices"/>
                    <xsd:element name="ActiveRecall"/>
                    <xsd:element name="ServiceHistory"/>
                    <xsd:element name="Items">
                        <xsd:complexType>   
                            <xsd:sequence>
                                <xsd:element name="Type"></xsd:element>
                                <xsd:element name="ENUM"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </xsd:schema>