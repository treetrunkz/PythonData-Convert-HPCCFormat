    <xsd:schema
        xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">    
        <xsd:element name="Search">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="RecordType"/>
