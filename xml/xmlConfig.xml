                    <xsd:element name="Make"/>
                    <xsd:element name="Model"/>
                    <xsd:element name="Year"/>
                    <xsd:element name="FirstName"/>
                    <xsd:element name="LastName"/>
                    <xsd:element name="MiddleName"/>
                    <xsd:element name="Suffix"/>
                    <xsd:element name="Street"/>
                    <xsd:element name="City"/>
                    <xsd:element name="State"/>
                    <xsd:element name="ZipCode5"/>
                    <xsd:element name="ZipCode4"/>
                    <xsd:element name="Phone1"/>
                    <xsd:element name="Phone2"/>
                    <xsd:element name="EmailAddress"/>
                    <xsd:element name="BuildSheet"/>
